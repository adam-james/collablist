<h2><%= @playlist.name %></h2>

<a href="<%= Routes.dashboard_playlist_search_path(@conn, :index, @playlist) %>">
  Add Track
</a>

<h3>Tracks</h3>

<ul>
<%= for track <- @tracks do %>
  <li><%= track.name %></li>
  <ul>
  <%= for artist <- track.artists do %>
    <li><%= artist["name"] %></li>
  <% end %>
  </ul>
<% end %>
</ul>
